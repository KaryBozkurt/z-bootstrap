    "chromatic": "^6.0.5",
    "husky": "^7.0.4",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.12.0",
    "react-is": "^17.0.2",
    "react-router-dom": "^5.3.0",
    "rollup-plugin-postcss": "^4.0.1",
    "size-limit": "^6.0.3",
